{
  "manifest_version": 3,
  "name": "Tachyon",
  "version": "1.0",
  "description": "Browse the web faster than light. Injects Tachyon to all pages to increase page load speed.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "tachyonEngine.js"
      ]
    }
  ]
}